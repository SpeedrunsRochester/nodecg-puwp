{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "tracker": {
      "type": "object",
      "properties": {
        "enable": {"type":  "boolean", "default": false},
        "url": {"type": "string", "default": "https://tracker.powerupwithpride.org"},
        "eventId": {"type": "string", "default": "1"}
      }
    },
    "extraLayouts": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {"type": "string"},
          "code": {"type": "string"}
        }
      },
      "default": []
    }
  }
}
